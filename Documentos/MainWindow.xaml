<Window x:Class="Documentos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Documentos"
        mc:Ignorable="d"
        x:Name="Docs"
        ResizeMode="NoResize"
        SizeToContent="WidthAndHeight"
        Icon="IcoProyectDocument.ico"
        Title="Docs" Height="500" Width="800">
    <StackPanel>
        <Button x:Name="NuevoDocumentoButton" Margin="5" Padding="5" Content="Nuevo Documento" Click="NuevoDocumentoButton_Click"/>
        <Button x:Name="ConfiguracionButton"  Margin="5" Padding="5" Content="Configuración" Click="ConfiguracionButton_Click" />        
        
        
        
    </StackPanel>
</Window>
